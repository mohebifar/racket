{
  "name": "<%= lodash.slugify(lodash.humanize(appName)) %>",
  "version": "1.0.0",
  "description": "A React app generated with generator-racket",
  "main": "index.js",
  "scripts": {
    "test": "NODE_PATH=./src ./node_modules/karma/bin/karma run",
    "enzyme": "NODE_PATH=./src ./node_modules/karma/bin/karma start",
    "dev": "concurrently --kill-others \"npm run webpack-dev\" \"npm run start-dev\"",
    "webpack-dev": "NODE_ENV=development NODE_PATH=./src node webpack/webpack-dev-server.js",
    "start-dev": "NODE_ENV=development NODE_PATH=./src node ./bin/server.js",
    "start": "NODE_ENV=production NODE_PATH=./src node ./bin/server.js",
    "build": "node ./node_modules/.bin/webpack --colors --config ./webpack/prod.config.js",
    "eslint": "eslint ./src",
    "postinstall": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mohebifar/racket.git"
  },
  "keywords": [
    "react",
    "generator",
    "redux"
  ],
  "author": "Mohamad Mohebifar",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mohebifar/generator-react-redux/issues"
  },
  "homepage": "https://github.com/mohebifar/generator-react-redux#readme",
  "dependencies": {
    "babel-polyfill": "^6.8.0",
    "body-parser": "^1.15.1",
